{
    "name": "jupyterlab_toscmode",
    "version": "0.3.0",
    "description": "Adds a Showcase Mode to Jupyter Lab",
    "keywords": [
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
    ],
    "homepage": "https://github.com/toscm/toscmode",
    "bugs": {
        "url": "https://github.com/toscm/toscmode/issues"
    },
    "license": "BSD-3-Clause",
    "author": {
        "name": "Tobias Schmidt",
        "email": "tobias.schmidt331@gmail.com"
    },
    "files": [
        "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
        "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
        "src/**/*.{ts,tsx}",
        "schema/*.json"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "style": "style/index.css",
    "repository": {
        "type": "git",
        "url": "https://github.com/toscm/toscmode.git"
    },
    "scripts": {
        "build": "npm run build:lib && npm run build:labextension:dev",
        "build:prod": "npm run clean && npm run build:lib:prod && npm run build:labextension",
        "build:labextension": "jupyter labextension build .",
        "build:labextension:dev": "jupyter labextension build --development True .",
        "build:lib": "tsc --sourceMap",
        "build:lib:prod": "tsc",
        "clean": "npm run clean:lib",
        "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
        "clean:lintcache": "rimraf .eslintcache .stylelintcache",
        "clean:labextension": "rimraf jupyterlab_toscmode/labextension jupyterlab_toscmode/_version.py",
        "clean:all": "npm run clean:lib && npm run clean:labextension && npm run clean:lintcache",
        "install:extension": "npm run build",
        "watch": "run-p watch:src watch:labextension",
        "watch:src": "tsc -w --sourceMap",
        "watch:labextension": "jupyter labextension watch ."
    },
    "dependencies": {
        "@jupyterlab/application": "^4.2.5",
        "@jupyterlab/apputils": "^4.3.5",
        "@jupyterlab/cells": "^4.2.5",
        "@jupyterlab/notebook": "^4.2.5",
        "@jupyterlab/settingregistry": "^4.2.5",
        "@lumino/widgets": "^2.5.0",
        "react": "^18.3.1",
        "webpack": "^5.95.0"
    },
    "devDependencies": {
        "@jupyterlab/builder": "^4.0.0",
        "rimraf": "^5.0.1",
        "typescript": "^5.0.0",
        "npm-run-all": "^4.1.5"
    },
    "sideEffects": [
        "style/*.css",
        "style/index.js"
    ],
    "styleModule": "style/index.js",
    "publishConfig": {
        "access": "public"
    },
    "jupyterlab": {
        "extension": true,
        "outputDir": "jupyterlab_toscmode/labextension",
        "schemaDir": "schema"
    }
}
